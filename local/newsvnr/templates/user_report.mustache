<script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/2.4.0/jszip.min.js"></script>
<div class="full-flex">
	<div class="d-flex">
		<a class="btn btn-secondary mr-2 user-report-fuction disabled" data-href="{{confirm}}" id="" href="javascript:;" data-toggle="modal" data-target="#popup-user-report">{{#str}} confirm , local_newsvnr {{/str}}</a>
		<a class="btn btn-secondary mr-2 user-report-fuction disabled" id="btn-user-delete" href="javascript:;" >{{#str}} deletecomment , local_newsvnr {{/str}}</a>
		<a class="btn btn-secondary mr-2 user-report-fuction disabled" data-href="{{message}}" id="" href="javascript:;" data-toggle="modal" data-target="#popup-user-report">{{#str}} sendmessage , local_newsvnr {{/str}}</a>
		<a class="btn btn-secondary mr-2 user-report-fuction disabled" data-href="{{display}}" id="" href="javascript:;" data-toggle="modal" data-target="#popup-user-report">{{#str}} displayonweb , local_newsvnr {{/str}}</a>
		<a class="btn btn-secondary mr-2" id="exporttable" href="javascript:;">{{#str}} export , local_newsvnr {{/str}}</a>
		<a class="btn btn-secondary mr-2 user-report-fuction disabled" data-href="{{password}}" id="" href="javascript:;"data-toggle="modal" data-target="#popup-user-report" >{{#str}} mandatorychangepassword , local_newsvnr {{/str}}</a>
		<a class="btn btn-secondary mr-2 user-report-fuction disabled" data-href="{{group}}" id="" href="javascript:;" data-toggle="modal" data-target="#popup-user-report">{{#str}} addtocohort , local_newsvnr {{/str}}</a>
	</div>
	<div>
	  	<button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton">{{#str}} search , local_newsvnr {{/str}}</button>
	  	<button class="btn btn-secondary" type="button" id="resettable"><i class="fa fa-refresh" aria-hidden="true"></i></button>
	  	<div class="dropdown-menu report user-report d-none" aria-labelledby="dropdownMenuButton">
            <ul class="nav-tabs nav multi-tab" tab="searchuser" style="padding: 0 25px;">
                <li class="nav-item"><a href="javascript:void(0)" class="nav-link active" data-key="account">{{#str}} accountinfo , local_newsvnr {{/str}}</a></li>
                <li class="nav-item"><a href="javascript:void(0)" class="nav-link" data-key="department">{{#str}} orgstructure , local_newsvnr {{/str}}</a></li>
                <li class="nav-item"><a href="javascript:void(0)" class="nav-link" data-key="advanced">{{#str}} searchadvance , local_newsvnr {{/str}}</a></li>
            </ul>
            <div class="tab-content" tab="searchuser" style="padding: 1.25rem">
                <div data="account" class="tab-pane active">
                	<div class="row">
                    	<div class="form-group col-md-6">
						    <label>{{#str}} accountname , local_newsvnr {{/str}}</label>
						    <input placeholder="{{#str}} accountname , local_newsvnr {{/str}}" class="form-control mt-1" id="username">
						</div>
						<div class="form-group col-md-6">
							<label >{{#str}} email , local_newsvnr {{/str}}</label>
						    <input placeholder="{{#str}} enteremail , local_newsvnr {{/str}}" class="form-control mt-1" id="useremail">
						</div>
		              	<div class="form-group col-md-6">
						    <label>{{#str}} codeuser , local_newsvnr {{/str}}</label>
						    <input placeholder="{{#str}} enterusercode , local_newsvnr {{/str}}" class="form-control mt-1" id="usercode">
						</div>
						<div class="form-group col-md-6">
							<label >{{#str}} accountstatus , local_newsvnr {{/str}}</label>
						    <input placeholder="{{#str}} selectstatus , local_newsvnr {{/str}}" class="form-control mt-1" id="userstatus">
						</div>
					</div>
					<div class="form-check">
					   <input class="form-check-input" type="checkbox" id="isnotaccess">
					   <label class="form-check-label">{{#str}} accountnotaccess , local_newsvnr {{/str}}</label>
					</div>
					<div class="form-check">
					   <input class="form-check-input" type="checkbox" id="isnotmodify">
					   <label class="form-check-label">{{#str}} accountnotedit , local_newsvnr {{/str}}</label>
					</div>
	                <div class="text-right">
	            		<button type="button" class="btn btn-primary" id="searchaccount">{{#str}} search , local_newsvnr {{/str}}</button>
	            	</div>
                </div>
                <div data="department" class="tab-pane">
                	<div class="row">
                    	<div class="form-group col-md-6">
						    <label>{{#str}} orgstructuretypeid , local_newsvnr {{/str}}</label>
						    <input class="form-control mt-1" id="orgstructure_category">
						</div>
						<div class="form-group col-md-6">
							<label >{{#str}} orgstructure , local_newsvnr {{/str}}</label>
						    <input class="form-control mt-1" id="orgstructure">
						</div>
		              	<div class="form-group col-md-6">
						    <label>{{#str}} jobtitle , local_newsvnr {{/str}}</label>
						    <input class="form-control mt-1" id="orgstructure_jobtitle">
						</div>
						<div class="form-group col-md-6">
							<label>{{#str}} jobposition , local_newsvnr {{/str}}</label>
						    <input class="form-control mt-1" id="orgstructure_position">
						</div>
					</div>
	              	<div class="text-right">
	            		<button type="button" class="btn btn-primary" id="searchorgs">{{#str}} search , local_newsvnr {{/str}}</button>
	            	</div>
                </div>
                <div data="advanced" class="tab-pane">
                	<div class="row">
                    	<div class="form-group col-md-6">
						    <label>{{#str}} accountnotedit , local_newsvnr {{/str}}</label>
						    <input class="form-control mt-1" id="system_role">
						</div>
						<div class="form-group col-md-6">
							<label>{{#str}} roleincourse , local_newsvnr {{/str}}</label>
							<div class="form-row">
								<div class="col">
							    	<input class="form-control mt-1" id="course_role">
							    </div>
							    <div class="col">
							    	<input class="form-control mt-1" id="list_course">
							    </div>
							</div>
						</div>
						<div class="form-group col-md-12">
							<label>{{#str}} lastaccess , local_newsvnr {{/str}}</label>
							<div class="row">
								<div class="col-md-6">
							    	<input placeholder="{{#str}} fromday , local_newsvnr {{/str}}" class="form-control mt-1" id="start_timeaccess">
							    </div>
							    <div class="col-md-6">
							    	<input placeholder="{{#str}} today , local_newsvnr {{/str}}" class="form-control mt-1" id="end_timeaccess">
							    </div>
							</div>
						</div>
                    </div>
    
	                <div class="text-right">
	            		<button type="button" class="btn btn-primary" id="searchadvance">{{#str}} search , local_newsvnr {{/str}}</button>
	            	</div>
                </div>

            </div>
	  	</div>
	</div>
</div>
<div id="user_report" class="kendo-custom kendo-user-report mt-2"></div>
<div class="modal" id="popup-user-report">
	<div class="modal-dialog">
		<!-- Modal content-->
		<div class="modal-content">
			<div class="modal-header">
			    <h4 class="modal-title"></h4>
			    <button type="button" class="close" data-dismiss="modal">&times;</button>
			</div>
			<div class="modal-body pd-0">
			</div>
			<div class="modal-footer">
			</div>
		</div>
	</div>
</div>
{{#js}}
	$('#dropdownMenuButton').click(function() {
		$('.dropdown-menu').toggleClass('d-none');
	})
{{/js}}