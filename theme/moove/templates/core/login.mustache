<div class="row p-0">
  <div class="col-md-9 hidden-sm hidden-xs p-0">
    {{#loginbgimg}}
      <img src="{{loginbgimg}}" class="vh-100 w-100">
    {{/loginbgimg}}
    {{^loginbgimg}}
      <img src="/theme/moove/pix/bg.jpg" class="vh-100 w-100">
    {{/loginbgimg}}
 </div>
 <div class="col-md-3 col-sm-12 col-xs-12 p-0 bg-white">
   <div class="p-0 ">
      <div class="card" style="border: none; box-shadow: none;">
         <div class="card-block">
            {{# logourl }}
            <h2 class="card-header text-center bg-transparent mb-3 p-4"><img src="{{logourl}}" title="{{sitename}}" alt="{{sitename}}" class="img-fluid"/> </h2>
            {{/ logourl }}
           {{#error}}
              <div class="loginerrors">
                  <a href="#" id="loginerrormessage" class="accesshide">{{error}}</a>
                  <div class="alert alert-danger" role="alert">{{error}}</div>
              </div>
            {{/error}}
               <div class="row justify-content-md-center">
                  <div class="col-md-12 p-0">
                     <h4 class="mb-4">{{#str}}login{{/str}}</h4>
                     <form action="{{loginurl}}" method="post" id="login" class="signup-form">
                        <input id="anchor" type="hidden" name="anchor" value="">
                        <script>document.getElementById('anchor').value = location.hash;</script>
                        <input type="hidden" name="logintoken" value="{{logintoken}}">
                        <label for="username" class="sr-only">
                        {{^canloginbyemail}}
                        {{#str}} username {{/str}}
                        {{/canloginbyemail}}
                        {{#canloginbyemail}}
                        {{#str}} usernameemail {{/str}}
                        {{/canloginbyemail}}
                        </label>
                        <div class="input-group input-group-username mb-3">
                           <input type="text" name="username" id="username"
                              class="form-control"
                              value="{{username}}"
                              placeholder={{#quote}}{{^canloginbyemail}}{{#str}}username{{/str}}{{/canloginbyemail}}{{#canloginbyemail}}{{#str}}usernameemail{{/str}}{{/canloginbyemail}}{{/quote}}>
                        </div>
                        <label for="password" class="sr-only">{{#str}} password {{/str}}</label>
                        <div class="input-group input-group-password mb-3">
                           <input type="password" name="password" id="password" value=""
                              class="form-control"
                              placeholder={{#quote}}{{#str}}password{{/str}}{{/quote}}>
                        </div>
                    <button type="submit" class="btn btn-primary btn-block rounded" id="loginbtn">{{#str}}login{{/str}}</button>
                     </form>
                  </div>
                  <div class="col-md-12 p-0">
                     <footer class="page-copyright py-4">
                        <p class="text-center text-muted">{{#str}}loginfollow, theme_moove{{/str}}</p>
                        <div class="social text-center">
                          {{#youtube}}
                           <a href="{{{youtube}}}" class="btn btn-icon btn-round social-facebook p-0 mr-1">
                            {{#youtubeicon}}
                              <img src="{{youtubeicon}}">
                            {{/youtubeicon}}
                            {{^youtubeicon}}
                              <img src="/theme/moove/pix/youtube.png">
                            {{/youtubeicon}}
                          </a>
                          {{/youtube}}
                          {{#facebook}} 
                           <a href="{{{facebook}}}" class="btn btn-icon btn-round social-twitter p-0 mr-1">
                            {{#facebookicon}}
                              <img src="{{facebookicon}}"></i>
                            {{/facebookicon}}
                            {{^facebookicon}}
                              <img src="/theme/moove/pix/facebook.png">
                            {{/facebookicon}}
                          </a>
                          {{/facebook}} 
                          {{#linkedin}} 
                           <a href="{{{linkedin}}}" class="btn btn-icon btn-round social-linkedin p-0 mr-1">
                            {{#linkedinicon}}
                              <img src="{{linkedinicon}}">
                            {{/linkedinicon}}
                            {{^linkedinicon}}
                              <img src="/theme/moove/pix/linkedin.png">
                            {{/linkedinicon}}
                          </a>
                          {{/linkedin}} 
                        </div>
                     </footer>
                  </div>
               </div>
         </div>
      </div>
   </div>
 </div>
 </div>
<script type="text/javascript">
   const submitform = (username, password) => {
       var username = username;
       var password = password;
        document.getElementById('username').value = '';
        document.getElementById('password').value = '';
       var intervalID = setInterval(function() {
           if(document.getElementById('username').className != 'complete'){
               var val = document.getElementById('username').value;
               if(val.length == username.length){
                   document.getElementById('username').classList.add("complete");
                   if(document.getElementById('password').className != 'complete'){
                       var val = document.getElementById('password').value;
                       if(val.length == password.length){
                           document.getElementById('password').classList.add("complete");
                       }else{
                           for(var i = 0; i < password.length; i++){
                               if(val.length <= i){
                                   document.getElementById('password').value = val + password.charAt(i);
                                   return;
                               }
                           }
                       }
                   }
                   var x = document.getElementsByClassName('signup-form');
                   x[0].submit();
                   clearInterval(intervalID);
               }else{
                   for(var i = 0; i < username.length; i++){
                       if(val.length <= i){
                           document.getElementById('username').value = val + username.charAt(i);
                           return;
                       }
                   }
               }
           }
           
   
       }, 100);
   }
   
</script>