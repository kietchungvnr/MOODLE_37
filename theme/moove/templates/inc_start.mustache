<style type="text/css">
    .qr-code {
        display: flex;
        margin: auto;
        padding: 10px;
        border-radius: 15px;
        border: 2px solid;
        border-color: #d5e1ea;
        margin: 16px 0px;
        justify-content: center;
    }
    .qr-code-icon {
        height: 24px;
        width: 24px;
        filter: contrast(0%);
    }
    .qr-code-icon:hover,
    .qr-code-icon:focus {
        filter: invert(27%) sepia(51%) saturate(2878%) hue-rotate(346deg) brightness(104%) contrast(97%);
    }
    .list-unstyled li a {
        color: #000;
    }
    #page-admin-search .card-body {
        margin-top: -1px;
    }
    #page-admin-search .title-admin,#page-admin-search .title-admin a {
        font-size: 15px;
        font-weight: bold;
    }
    #page-admin-search .nav.nav-tabs.course .nav-link.active {
        border-bottom: 0 !important;
        color: #363535 !important;
        background: white !important;
    }
    #page-admin-search .nav-tabs.course .nav-link.active {
        box-shadow: unset;
    }
    #page-mod-quiz-edit .section-heading {
        font-size: 14px;
        display: flex;
        align-items: center;
        justify-content: space-between;
    }
    #page-mod-quiz-edit .section-heading .instanceshufflequestions {
        margin: 0;
    }
    #page-mod-quiz-edit .section-heading .instancesectioncontainer {
        font-size: 15px;
    }
    #page-mod-quiz-edit ul.slots li.section .content {
        background: transparent;
    }
    #page-mod-quiz-edit .title-edit-question h2 {
        font-size: 14px !important;
        color:#363535;
    }   
    .path-mod-quiz .mod-quiz-edit-content {
        margin-bottom: 0;
    }
    #page-mod-quiz-edit #displayoptions {
        border: 0;
        padding: 0;
    }
    #page-mod-quiz-edit #categoryquestions>tbody>tr:nth-of-type(even) {
        background: white
    }
    #page-mod-quiz-edit .modal-dialog {
        max-width: 70%;
    }
    #page-mod-quiz-edit .fcontainer {
        border: 0;
        padding: 0;
    }
    #page-mod-quiz-edit .modal-footer {
        display: flex;
        justify-content: center;
        border-top: 0;
    }
    #page-mod-quiz-edit .modal-footer.hidden {
        display: none;
    }
    #page-mod-quiz-edit .modulespecificbuttonscontainer {
        text-align: center;
    }
    #page-mod-quiz-edit .iconcol.previewaction a i{
        margin-right: 10px;
    }
    #page-mod-quiz-edit #categoryquestions .iconcol .icon {
        margin-top: 5px;
    }
    #page-mod-quiz-edit #fitem_id_fromtags .form-autocomplete-selection {
        display: block;
        text-align: left;
    }
    #page-mod-quiz-edit div[data-region="paging-control-container"] {
        justify-content: flex-end;
    }
    #page-mod-quiz-edit .mod_quiz-edit-top-controls {
        padding: 0 0 0 45px;
    }
    .page-link:focus {
        box-shadow: unset;
    }
    #page-mod-quiz-edit .paging {
        text-align: left;
        background: transparent;
    }
    #page-mod-quiz-edit .questionbankformforpopup .form-group {
        display: none;
    }
    @media(max-width: 1200px) {
        .k-widget.k-window {
            width: 98vw !important;
        }
    }
    .competencylinktree h3 {
        font-size: 14px;
    }
    .path-admin-tool-lp [data-region="competencylinktree"]>ul {
        height: 150px;
    }
</style>
