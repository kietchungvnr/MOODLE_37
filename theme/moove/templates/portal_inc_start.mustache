    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
 
<style type="text/css">
 	#page.container-fluid {
 		padding: 0 !important;
 	}
 	nav.fixed-top.navbar.focusmod {
 		display: none !important;
 	}
 	#region-main-box {
 		margin-top: 0 !important;
 	}
 	#sidepreopen-control {
 		display: none;
 	}
 	.quiz-menu-bottom {
 		bottom: 0;
 	}
 	#region-main {
 		background: white;
 	}
 	.card {
 		border-radius: 0;
 	}
 	body {
 		overflow-x: hidden;
 	}
	@media (max-width: 576px) {
        .d-flex-desktop {
            display: block;
        }
        body {
            background-color: #ffffff;
        }
        .action-menu-trigger {
            display: none;
        }
        .questionflag {
            display: none;
        }
        .editquestion {
            display: none;
        }
        .mod_quiz-prev-nav {
            display: none;
        }
        .mod_quiz-next-nav {
            display: none;
        }
        #page-mod-quiz-attempt #sidepreopen-control,
        #page-mod-quiz-attempt #sidepre-blocks {      
            display: none;
        } 
        #page-mod-quiz-attempt .card-body {
            background: #f7f7f7;
        }
        #page-mod-quiz-attempt .submitbtns {
            height: 35px;
            margin-bottom: 60px;
        }
        #page-mod-quiz-attempt .que {
            padding: 0;
            background: #f7f7f7;
        }   
        #page-mod-quiz-attempt .que .qtext {
            font-size: 15px;
        }
        #page-mod-quiz-attempt #page-wrapper {
            margin-top: 40px;
        }
        #page-mod-quiz-attempt .que.multichoice .formulation .r1 input[type="radio"],
        #page-mod-quiz-attempt .que.multichoice .formulation .r0 input[type="radio"] {
            display: none;
        }
        #page-mod-quiz-attempt .que.multichoice label {
            font-size: 15px;
            align-items: center;
        }
        .que span.qno {
            display: none;
        }
        .que img.icon {
            width: auto !important;
        }
        .number-question-mb {
            font-size: 15px;
        }
        .que.multichoice .answer div.r0,
        .que.multichoice .answer div.r1 {
            border: none;
            background: white;
            background: linear-gradient(to right, #007bff 50%, white 50%);
            background-size: 201% 100%;
            background-position: right bottom;
            transition: all ease 0.3s;
        }
        .que.multichoice .answer div.r0.active,
        .que.multichoice .answer div.r1.active {
            background-position: left bottom;
            color: white;
            transition: all ease 0.3s;
        }
        .que.multichoice .answer div.r0.active .answernumber,
        .que.multichoice .answer div.r1.active .answernumber {
            visibility: hidden;
        }
        .que.multichoice .formulation .answer .answernumber {
            text-transform: uppercase;
            width: 25px;
            height: 25px;
            text-align: center;
            border-radius: 50%;
            margin-right: 5px;
            background: #007bff;
            color: white;
            font-weight: bold;
            padding: 2px;
        } 
        .que.multichoice .clear-choice {
            height: 25px;
            text-align: center;
            padding: 3px;
            border-radius: 50%;
            color: #007bff;
            background: white;
            width: 27px;
            font-size: 18px;
            display: none;
        } 
        .que.truefalse .answer div.r0,
        .que.truefalse .answer div.r1 {
            border-radius: 8px;
            background: white;
            background: linear-gradient(to right, #007bff 50%, white 50%);
            background-size: 201% 100%;
            background-position: right bottom;
            transition: all ease 0.3s;
        }
        .qtype_multichoice_clearchoice {
            display: none;
        }
        #page-mod-quiz-view .moodle-dialogue-focused {
            left: 15px !important;
            top: 100px !important;
            z-index: 1005;
            right: 15px !important;
            width: auto;
        }
        #page-mod-quiz-view #mod_quiz_preflight_form {
            margin-top: 15px;
        }
        .formulation img {
            width: 100%;
        }
        body#page-mod-quiz-attempt {
            background: #f7f7f7;
        }
        #page-mod-quiz-review {
        	padding-top: 0px;
        }
	    .modal-dialog[role="document"] {
    	    max-width: inherit;
		    height: 100%;
        }
        .modal-dialog[role="document"] .modal-content {
        	padding: 0;
        	border-radius: 0;
        }
        #page-course-view-topcoll .back-focusmod.toolportal {
		    position: fixed;
		    display: flex;
		    background: #3c8dbc;
		    left: 8px;
		    right: 0;
		    bottom: 35px;
		    color: red;
		    width: 30px;
		    height: 30px;
		    z-index: 123;
		    color: white;
		    text-align: center;
		    align-items: center;
		}
		body.path-course-view::-webkit-scrollbar {
		    display: none;
		}
		.dot-desktop {
			display: none !important;
		}
    }

</style>
