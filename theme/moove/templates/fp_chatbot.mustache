<div id="chatbot-chat">
    <div class="app-wrapper app-responsive">
        <div data-mobile="false" class="opened">
            <div class="chat">
                <div class="header-chatbot">
                    <div class="close">
                        <div data-size="normal" class="tpl-close">
                            <svg viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                                <path d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z" fill="#fff"></path>
                            </svg>
                        </div>
                    </div> 
                    <div class="avatar">
                        <div class="tpl-avatar">
                            <div class="tpl-avatar-status" style="border-color: rgb(255, 255, 255); background: rgb(105, 222, 64);"></div> 
                            <div class="tpl-avatar-image">
                                <div data-status="loaded" data-cover="true" class="lazy-img bg-white">
                                    <img src="/theme/moove/pix/chatbot-icon.png" alt="" class="lazy-img-loaded">
                                </div>
                            </div>
                        </div>
                    </div> 
                    <div class="company">
                        <div class="header">
                            Trợ lý E-Learning
                        </div> 
                        <div class="status" style="color: rgb(155, 166, 179);">
                            Trực tuyến
                        </div> 
                    </div>
                </div>
                <div class="conversation-chatbot">
                    <div>
                    <div class="main-conversation-chatbot">
                    </div>
                    </div>
                    {{! <div class="bot-caption d-none" style="position: absolute;bottom: 45px;">
                        <span class="tag-chatbot" id="help-tag">Trợ giúp</span> 
                    </div> }}
                </div>
                <div class="typing-chatbot">
                    <input type="text" maxlength="256" placeholder="Type your message here" style="color: rgb(150, 155, 166);" id="send-message" name="send-message">
                    <div class="send-icon">
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 24 24" xml:space="preserve">
                            <path fill="#d7d7d7" d="M22,11.7V12h-0.1c-0.1,1-17.7,9.5-18.8,9.1c-1.1-0.4,2.4-6.7,3-7.5C6.8,12.9,17.1,12,17.1,12H17c0,0,0-0.2,0-0.2c0,0,0,0,0,0c0-0.4-10.2-1-10.8-1.7c-0.6-0.7-4-7.1-3-7.5C4.3,2.1,22,10.5,22,11.7z"></path>
                        </svg>
                    </div>
                </div>
            </div>
           
        </div>
    </div>
    <div class="view-detail-popup">
                <div class="header-popup align-items-center d-flex header-popup">
                    <span class="header-popup-result">Kết quả:</span>
                </div>
                <div class="content-popup">
                    <div class="" id="report-chatbot"></div>
                </div>
                <div class="footer-popup">
                    
                </div>
            </div>
   
</div>
