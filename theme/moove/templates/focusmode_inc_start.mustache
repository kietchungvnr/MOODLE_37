<head>
    <title>{{{ output.page_title }}}</title>
    <link rel="shortcut icon" href="{{{ output.favicon }}}" />
    
    {{{ output.standard_head_html }}}
    
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">

    <link rel="stylesheet" href="https://kendo.cdn.telerik.com/2020.3.915/styles/kendo.common-material.min.css" />
<style type="text/css">
    #sidepre-blocks {
        top: calc(100% - 100vh);
        height: 100%;
    }
    #page-course-view-topics .defaultuserpic {
        width: 30px !important;
        height: 30px !important;
    }
    #page.container-fluid {
        padding: 0rem 1.5rem 0 1.5rem !important;
    }
    #region-main {
        border: 0 !important;
    }
    #page-mod-quiz-attempt #quiz-timer {
        display: block;
    }
    #page-wrapper {
        margin-left: 0;
    }
    .icon-opensection-mb {
        display: none;
    }
    .que.essay .formulation .qtext {
        display: none;
    }
    .formulation { 
        color: black;
    }
    #page-content {
        margin-top: 10px;
    }
    @media (max-width: 576px) {
        .d-flex-desktop {
            display: block;
        }
        body {
            background-color: #ffffff;
        }
        .action-menu-trigger {
            display: none;
        }
        .questionflag {
            display: none;
        }
        .editquestion {
            display: none;
        }
        .mod_quiz-prev-nav {
            display: none;
        }
        .mod_quiz-next-nav {
            display: none;
        }
        #page-mod-quiz-attempt #sidepreopen-control,
        #page-mod-quiz-attempt #sidepre-blocks {      
            display: none;
        } 
        #page-mod-quiz-attempt .card-body {
            background: #f7f7f7;
        }
        #page-mod-quiz-attempt .submitbtns {
            height: 35px;
            margin-bottom: 60px;
        }
        #page-mod-quiz-attempt .que {
            padding: 0;
            background: #f7f7f7;
        }   
        #page-mod-quiz-attempt .que .qtext {
            font-size: 15px;
        }
        #page-mod-quiz-attempt #page-wrapper {
            margin-top: 40px;
        }
        #page-mod-quiz-attempt .que.multichoice .formulation .r1 input[type="radio"],
        #page-mod-quiz-attempt .que.multichoice .formulation .r0 input[type="radio"] {
            display: none;
        }
        #page-mod-quiz-attempt .que.multichoice label {
            font-size: 15px;
            align-items: center;
        }
        .que span.qno {
            display: none;
        }
        .que img.icon {
            width: auto !important;
        }
        .number-question-mb {
            font-size: 15px;
        }
        .que.multichoice .answer div.r0,
        .que.multichoice .answer div.r1 {
            border: none;
            background: white;
            background: linear-gradient(to right, #007bff 50%, white 50%);
            background-size: 201% 100%;
            background-position: right bottom;
            transition: all ease 0.3s;
        }
        .que.multichoice .answer div.r0.active,
        .que.multichoice .answer div.r1.active {
            background-position: left bottom;
            color: white;
            transition: all ease 0.3s;
        }
        .que.multichoice .answer div.r0.active .answernumber,
        .que.multichoice .answer div.r1.active .answernumber {
            visibility: hidden;
        }
        .que.multichoice .formulation .answer .answernumber {
            text-transform: uppercase;
            width: 25px;
            height: 25px;
            text-align: center;
            border-radius: 50%;
            margin-right: 5px;
            background: #007bff;
            color: white;
            font-weight: bold;
            padding: 2px;
        } 
        .que.multichoice .clear-choice {
            height: 25px;
            text-align: center;
            padding: 3px;
            border-radius: 50%;
            color: #007bff;
            background: white;
            width: 27px;
            font-size: 18px;
            display: none;
        } 
        .que.truefalse .answer div.r0,
        .que.truefalse .answer div.r1 {
            border-radius: 8px;
            background: white;
            background: linear-gradient(to right, #007bff 50%, white 50%);
            background-size: 201% 100%;
            background-position: right bottom;
            transition: all ease 0.3s;
        }
        .qtype_multichoice_clearchoice {
            display: none;
        }
        #page-mod-quiz-view .moodle-dialogue-focused {
            left: 15px !important;
            top: 100px !important;
            z-index: 1005;
            right: 15px !important;
            width: auto;
        }
        #page-mod-quiz-view #mod_quiz_preflight_form {
            margin-top: 15px;
        }
        .formulation img {
            width: 100%;
        }
        body#page-mod-quiz-attempt {
            background: #f7f7f7;
        }
        body #page-wrapper {
            padding: 0
        }
    }
    @media (max-width: 950px) {
        li.nav-item.border-left {
            display: none;
        }
    }
</style>
